@PACKAGE_INIT@

get_filename_component(graph_core_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

find_dependency(Eigen3     REQUIRED)
find_dependency(Boost      REQUIRED COMPONENTS date_time filesystem)
find_dependency(PkgConfig  REQUIRED)
find_dependency(cnr_logger REQUIRED)
find_dependency(yaml-cpp   REQUIRED)

include("${graph_core_CMAKE_DIR}/graph_coreTargets.cmake")

set(graph_core_FOUND TRUE)

set(graph_core_INCLUDE_DIRS 
  "@CONF_INCLUDE_DIRS@"  # this is the package include folder
  ${EIGEN3_INCLUDE_DIRS}
  ${cnr_logger_INCLUDE_DIRS}
  ${YAML_CPP_INCLUDE_DIRS}
  ${Boost_INCLUDE_DIRS}
  )
  
set(graph_core_LIBRARIES
  graph_core # libraries defined in this package (in this case, there is one)
  ${YAML_CPP_LIBRARIES}
  ${cnr_logger_LIBRARIES}
  ${Boost_LIBRARIES})

check_required_components(graph_core)
