<?xml version="1.0"?>
<launch>

  <rosparam command="load" file="$(find graph_replanning)/config/object_test_replanner.yaml"/>
  <rosparam command="load" file="$(find graph_replanning)/config/test_online_replanner_panda.yaml"/>
  <include file="$(find cell_moveit_config)/launch/demo.launch"/>
  <node pkg="object_loader" name="context_manager_node" type="context_manager_node" respawn="true" output="screen"/>

  <rosparam command="load" file="$(find graph_replanning)/config/static_scene_panda.yaml"/>
  <node pkg="object_loader" name="scene_spawner" type="scene_spawner" output="screen"/>

  <node name="test_online_replanner"
        pkg="graph_replanning"
        type="test_online_replanner"
        output="screen"
        launch-prefix="gdb -ex run --args" >
  </node>

</launch>
